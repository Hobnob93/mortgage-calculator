<MudCard Style="@CardStyling" Class="@CardClasses">
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h5">@Title</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent Class="@CardContentClasses">
        <MudPaper Elevation="0">
            @ChildContent
        </MudPaper>
    </MudCardContent>
</MudCard>
