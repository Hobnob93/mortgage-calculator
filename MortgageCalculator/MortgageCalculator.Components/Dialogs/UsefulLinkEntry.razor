<MudDialog>
    <DialogContent>
        Useful Link: @UsefulLink.Name
    </DialogContent>

    <DialogActions>
        <MudButton OnClick="() => Dialog.Cancel()" Color="Color.Error">Close</MudButton>
        <MudButton OnClick="() => Dialog.Close(DialogResult.Ok(UsefulLink))" Color="Color.Success">Save Changes</MudButton>
    </DialogActions>
</MudDialog>